{
  "name": "root",
  "version": "0.0.0",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "start": "dumi dev",
    "bootstrap": "pnpm i && lerna bootstrap",
    "build": "lerna run build",
    "test": "lerna run test --stream",
    "test:cov": "lerna run test:cov --stream",
    "pub": "npm run build && lerna publish",
    "docs:build": "dumi build && node ./scripts/add_base.js",
    "analyze": "cross-env ANALYZE=1 dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "pub:doc": "npm run docs:build && npm run docs:deploy"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.15.1",
    "@testing-library/react": "^12.1.2",
    "@types/jest": "^27.0.3",
    "@types/lodash": "^4.14.180",
    "@types/lodash.set": "^4.3.6",
    "@types/url-parse": "^1.4.8",
    "@umijs/test": "^3.0.5",
    "antd": "^4.17.3",
    "cross-env": "^7.0.3",
    "dumi": "^1.0.17",
    "father-build": "^1.17.2",
    "gh-pages": "^3.0.0",
    "jest": "^27.4.4",
    "lerna": "^4.0.0",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "ts-jest": "^27.1.1",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "dayjs": "^1.11.0",
    "decimal.js": "^10.3.1",
    "lodash": "^4.17.21",
    "url-parse": "^1.5.10"
  }
}
