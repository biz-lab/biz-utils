{
  "name": "root",
  "version": "0.0.0",
  "scripts": {
    "start": "dumi dev",
    "exp": "cd packages/examples && pnpm run exp",
    "exp:init": "cd packages/examples && pnpm i",
    "exp:build": "cd packages/examples && pnpm run build",
    "bootstrap": "pnpm i && lerna bootstrap",
    "build": "lerna run build --stream",
    "docs": "lerna run docs --stream",
    "test": "lerna run test --stream",
    "test:cov": "lerna run test:cov --stream",
    "pub": "npm run build && lerna publish",
    "tsc": "lerna run tsc --stream",
    "analyze": "cross-env ANALYZE=1 dumi build",
    "docs:build": "dumi build && node ./scripts/add_base.js",
    "docs:apis": "npm run docs && node ./scripts/copy_apis.js",
    "docs:deploy": "gh-pages -d docs-dist",
    "pub:doc": "npm run docs:build && npm run docs:apis && npm run docs:deploy"
  },
  "devDependencies": {
    "@types/file-saver": "^2.0.5",
    "@types/jest": "^29.0.3",
    "@types/lodash": "^4.14.180",
    "@types/react": "^18.0.9",
    "@types/uuid": "^8.3.4",
    "@types/validator": "^13.7.2",
    "antd": "^4.17.3",
    "cross-env": "^7.0.3",
    "father": "^4.0.5",
    "fs-extra": "^10.1.0",
    "gh-pages": "^3.0.0",
    "jest": "^29.0.3",
    "jest-environment-jsdom": "^29.0.3",
    "lerna": "^5.5.0",
    "lint-staged": "^10.0.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "ts-jest": "^29.0.1",
    "typedoc": "^0.22.15",
    "typescript": "^4.8.3"
  }
}
