{
  "name": "root",
  "version": "0.0.0",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "start": "dumi dev",
    "exp": "cd packages/examples && pnpm run exp",
    "bootstrap": "pnpm i && lerna bootstrap",
    "build": "lerna run build",
    "test": "lerna run test --stream",
    "test:cov": "lerna run test:cov --stream",
    "pub": "npm run build && lerna publish",
    "docs:build": "dumi build && node ./scripts/add_base.js",
    "analyze": "cross-env ANALYZE=1 dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "pub:doc": "npm run docs:build && npm run docs:deploy"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.2",
    "@types/file-saver": "^2.0.5",
    "@types/jest": "^27.0.3",
    "@types/lodash": "^4.14.180",
    "@types/url-parse": "^1.4.8",
    "@types/uuid": "^8.3.4",
    "@types/validator": "^13.7.2",
    "@umijs/test": "^3.0.5",
    "antd": "^4.17.3",
    "clipboard": "^2.0.10",
    "cross-env": "^7.0.3",
    "dumi": "^1.0.17",
    "father-build": "^1.21.2",
    "gh-pages": "^3.0.0",
    "jest": "^27.4.4",
    "lerna": "^4.0.0",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rollup-plugin-commonjs": "^10.1.0",
    "ts-jest": "^27.1.1",
    "typescript": "^4.4.4",
    "uuid": "^8.3.2",
    "validator": "^13.7.0"
  }
}
